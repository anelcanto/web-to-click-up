{
    "manifest_version": 3,
    "name": "Web-to-ClickUp! BETA",
    "version": "1.22",
    "description": "Boost productivity by creating ClickUp tasks instantly. Turn any website content into ClickUp tasks with just one click!",
    "permissions": [
        "storage",
        "sidePanel",
        "tabs",
        "identity"
    ],
    "action": {
        "default_title": "Click to open panel"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "https://api.clickup.com/"
    ],
    "side_panel": {
        "default_path": "index.html"
    },
    "oauth2": {
        "client_id": "KHYFIN9VSU8TBIRSA10WOGT5BPSQI0TO",
        "scopes": []
    },
    "key": "",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+E"
            },
            "description": "Open extension sidebar"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}