{
    "manifest_version": 3,
    "name": "Web-to-ClickUp! BETA",
    "version": "1.3",
    "description": "Boost your productivity by creating ClickUp tasks instantly without leaving your current tab. Easily convert content from any website into actionable tasks in ClickUp with just a click!",
    "permissions": [
        "storage",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://api.clickup.com/*"
    ],
    "action": {
        "default_title": "Click to open panel"
    },
    "background": {
        "service_worker": "background.js"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+E"
            },
            "description": "Open extension sidebar"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}